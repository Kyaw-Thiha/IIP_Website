---
export interface Props {
  src: string;
  title?: string;
  width: string;
  height: string;
}

const { src, title, width, height } = Astro.props;
---

<iframe
  src={src}
  title={title}
  width={width}
  height={height}
  style="border:0;"
  allowfullscreen=""
  loading="lazy"
  referrerpolicy="no-referrer-when-downgrade"
>
</iframe>
