---
export interface Props {
  src: string;
  title?: string;
  class?: string;
  width?: string;
  height?: string;
}

const { src, title, class: className, width, height } = Astro.props;
---

<iframe
  src={src}
  title={title}
  width={width}
  height={height}
  class={className}
  style="border:0;"
  allowfullscreen=""
  loading="lazy"
  referrerpolicy="no-referrer-when-downgrade"
>
</iframe>
